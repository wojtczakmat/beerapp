<%= form_for :event, url: events_path do |f| %>
    <p>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </p>
    <p>
        <%= f.label :date %><br>
        <%= f.date_field :date %>
    </p>
    <p>
        <%= f.label :time %><br>
        <%= f.time_field :time %>
    </p>
    <p>
        <%= f.label :description %><br>
        <%= f.text_area :description %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>
<%= link_to 'Back', events_path %>

